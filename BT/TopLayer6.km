    else if intX <= -38 and tarX >= -38 then //目的在右,人在左
        SavedX		= tarX
        SavedY		= tarY
        SavedTor	= tolerance
        SavedTorY	= yTolerance

        tarX      = -85
        tarY      = 117
        tolerance = 0
        alterX    = 1 //传送位置
        alterY    = 67
        Call Move_Bear()

        if tarY < 91 then //人在右上,目标在右下,再传一次(右下位置)
            tarX      = 1
            tarY      = 67
            tolerance = 0
            alterX    = 1 //传送位置
            alterY    = 115
            Call Move_Bear()
        End If

        tarX		= SavedX
        tarY		= SavedY
        tolerance	= SavedTor
        yTolerance	= SavedTorY
    else if tarX <= -38 and intX >= -38 then //目的在左边
        SavedX		= tarX
