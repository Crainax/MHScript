        if GetTimeStamp() - pick3 > 120 then
            tarX      = 1
            tarY      = 115
            tolerance = 6
            Call Move_Bear()
            DelayRandom 42,50
            KeyDown Left
            DelayRandom 161,163
            KeyDown Space
            DelayRandom 78,79
            KeyUp Space
            DelayRandom 251,254
            KeyDown Space
            DelayRandom 104,106
            KeyUp Space
            DelayRandom 582,588
            KeyDown Space
            DelayRandom 70,71
            KeyUp Space
            DelayRandom 186,188
            KeyDown Space
            DelayRandom 111,113
            KeyUp Space
            DelayRandom 739,747
            KeyDown Space
            DelayRandom 95,96
            KeyUp Space
            DelayRandom 161,163
            KeyDown Space
            DelayRandom 86,87
            KeyUp Space
            DelayRandom 195,197
            KeyUp Left
            DelayRandom 325,329
            pick3 = GetTimeStamp()
        else
            tarX      = 1
            tarY      = 115
            tolerance = 0
            alterX    = -85 //传送位置
            alterY    = 117
            Call Move_Bear()
        End If

        tarX      = SavedX
        tarY      = SavedY
        tolerance = SavedTor
    End If
    Call Move_Bear()
End Sub

Sub on_timer4 //查地图轮,每3秒一次 ver2.0不需要存储了
    a = GetTimeStamp() - RuneCooldown
    if a > 600 then //可以放轮了
        FindPic x1,y1,x2,y2,"E:\MHImg\Rune.bmp",000000,1.00,0,intX,intY
        if intX >= 0 then
            FindPic x1,y1,x2,y2,"E:\MHImg\MapAnchor.bmp",000000,1.00,0,logoX,logoY
            intX = intX - logoX
            intY = intY - logoY - 1 //图标的对准问题
            tarX       = intX
            tarY       = intY
            tolerance  = 2
            yTolerance = 3
            KeyAllup //防错误
            Call Navigate() //解轮也走捷径
            //暂停脚本
            Beep 1000,200
            DelayRandom 100,120
            Beep 1000,200
            DelayRandom 100,120
            Beep 1000,200
            DelayRandom 100,120
            Beep 1000,200
            Pause
            call GetXY()
            tarX = intX
            tarY = intY
            RuneCooldown = GetTimeStamp()
        else
        End If
        Timer 4,1,5000 //无限循环
    End If
EndSub

Sub Fart //放熊屁,CD没好就放其他技能 Ver2.2 这个有武功
    while 1 > 0

        FindPic x3,y3,xEnd,yEnd,"E:\MHImg\Lynn\Old\W2.bmp",000000,1.00,0,intX,intY	//找V2
        if intX >= 0 Then //找到V2就放,瞬手的事
            KeyDown W
            DelayRandom 122,124
            KeyUp W
            DelayRandom 402,412
        End If

        FindPic x3,y3,xEnd,yEnd,"E:\MHImg\Lynn\Old\D.bmp",000000,1.0,0,intX,intY	//找熊屁
        if intX >= 0 or FartOK == 1 Then
            FartOK = 0
            Timer 1,1,5000
            KeyDown D
            DelayRandom 623,650
            KeyUp D
            DelayRandom 105,106
            break
        else //放其他小技能
            FindPic x3,y3,xEnd,yEnd,"E:\MHImg\Common\H.bmp",000000,1.00,0,intX,intY
            if intX >= 0 Then //找蜘蛛
                KeyDown H
                DelayRandom 105,107
                KeyUp H
                DelayRandom 929,939
            else
                FindPic x3,y3,xEnd,yEnd,"E:\MHImg\Common\YA.bmp",000000,1.00,0,intX,intY
                If DarkNess < 0 and intX >= 0 Then //幽暗开关
                    KeyDown G
                    DelayRandom 104,106
                    KeyUp G
                    DelayRandom 971,981
                    DarkNess = 0
                else
                    FindPic x3,y3,xEnd,yEnd,"E:\MHImg\Common\Y.bmp",000000,1.00,0,intX,intY
                    if intX >= 0 Then //找烈阳
                        KeyDown Y
                        DelayRandom 105,107
                        KeyUp Y
                        DelayRandom 811,813
                    else
                        FindPic x3,y3,xEnd,yEnd,"E:\MHImg\Common\4.bmp",000000,1.00,0,intX,intY
                        a = GetTimeStamp() - WGTime
                        if intX >= 0 and a > 60 Then //找哨子
                            KeyDown 4
                            DelayRandom 105,107
                            KeyUp 4
                            DelayRandom 899,909
                            WGTime = GetTimeStamp()
                        else
                            FindPic x3,y3,xEnd,yEnd,"E:\MHImg\Lynn\Old\C.bmp",000000,1.00,0,intX,intY
                            a = GetTimeStamp() - WhisterTime
                            if intX >= 0 and a > 160 Then //找哨子
                                KeyDown 5
                                DelayRandom 105,107
                                KeyUp 5
                                DelayRandom 272,284
                                KeyDown B
                                DelayRandom 97,99
                                KeyUp B
                                DelayRandom 312,324
                                KeyDown C
                                DelayRandom 214,217
                                KeyUp C
                                DelayRandom 879,891
                                WhisterTime = GetTimeStamp()
                            End If
                        End If
                    End If
                End If
            End If
        End If

        DelayRandom 5,10
    Wend
    If DarkNess < 0 Then //幽暗开关
        FindPic x3,y3,xEnd,yEnd,"E:\MHImg\Common\YA.bmp",000000,1.00,0,intX,intY
        if intX >= 0 then
            KeyDown G
            DelayRandom 104,106
            KeyUp G
            DelayRandom 971,981
            DarkNess = 0
        End If
    End If
End Sub

