Sub GetXY //获取角色的XY轴(如果找不到会左右移动)
    FindPic x1,y1,x2,y2,"E:\MHImg\Me.bmp",000000,1.00,0,intX,intY
    FindPic x1,y1,x2,y2,"E:\MHImg\MapAnchor.bmp",000000,1.00,0,logoX,logoY

    if intX < 0 Then //人物跑到了边界  处理一下
        KeyDown Left
        DelayRandom 200,250
        KeyUp Left
        FindPic x1,y1,x2,y2,"E:\MHImg\Me.bmp",000000,1.00,0,intX,intY
        if intX < 0 Then //左移了还是没找到,就右移
            KeyDown Right
            DelayRandom 200,250
            KeyUp Right
            FindPic x1,y1,x2,y2,"E:\MHImg\Me.bmp",000000,1.00,0,intX,intY
        End if
    End if

    intX = intX - logoX
    intY = intY - logoY
End Sub

Sub AntiJam //一般跟随GetXY后用,  Ver2.0 延长了一点时间防误判断
    if staticX ==  intX and staticY == intY then //如果相等就累加计数器
        staticCount = staticCount + 1
    else
        staticX     = intX
        staticY     = intY
        staticCount = 0
    End If
    if staticCount >= 5 Then //
        Beep 500,500
        KeyAllup //施放所有按键并重启
        // RestartScript //重启 和上面二选一即可
        // 模拟从绳子跳走
        DelayRandom 13,15
        KeyDown Right
        DelayRandom 21,22
        KeyDown Space
        DelayRandom 76,77
        KeyUp Space
        DelayRandom 44,45
        KeyUp Right
        DelayRandom 13,15

    End If
End Sub

Sub Move //精确的移动:V2.9 定点传送门搞定了
    csTimes = -1 //初始就启用指南针判断
    stuckTimes = 0 //防卡检测初始次数为0
    cX = -100
    cY = 0
    KeyAllup //施放所有按键防止卡住
    while  1 > 0

        Call GetXY()
        if csTimes >= 0 Then //目前是在空中判断降落的阶段
            if intY == cY Then //Y轴没变化了
                csTimes = csTimes + 1
                if csTimes >= 8 Then
                    csTimes	= -1	//判断为已经落地
                End If
            else
                cY = intY
                csTimes = 0
            End If
        else //继续根据
            if tolerance > 4 Then
                tolLoc = tolerance
            else  //速走的默认为4
                tolLoc = 4
            End If
            if intX < (tarX-12) Then //右瞬:这里的shit必须要抬起来 不然好像下面会出问题
                If stuckTimes < 2 Then
                    if cX != intX Then //有变化了,就重置计数器
                        cX = intX
                        stuckTimes = 0
                    else
                        stuckTimes = stuckTimes + 1 //原地就计数器+1
                    End If
                    Call AntiJam()
                    Call Attack()
                    DelayRandom 115,117
                    KeyDown Right
                    DelayRandom 53,54
                    KeyDown LShift
                    DelayRandom 86,87
                    KeyUp LShift
                    DelayRandom 22,23
                    KeyUp Right
                    DelayRandom 356,360
                else //计数器达到5次Y轴不动了,下跳
                    DelayRandom 53,54
                    KeyDown Right
                    DelayRandom 186,187
                    KeyUp Right
                    DelayRandom 22,23
                    stuckTimes = 0
                    cX = -100
                End If
            Else If intX > (tarX+12) Then //向左瞬
                If stuckTimes < 2 Then
                    if cX != intX Then //有变化了,就重置计数器
                        cX = intX
                        stuckTimes = 0
                    else
                        stuckTimes = stuckTimes + 1 //原地就计数器+1
                    End If
                    Call AntiJam()
                    Call Attack()
                    DelayRandom 115,117
                    KeyDown Left
                    DelayRandom 53,54
                    KeyDown LShift
                    DelayRandom 86,87
                    KeyUp LShift
                    DelayRandom 22,23
                    KeyUp Left
                    DelayRandom 356,360
                else //计数器达到5次Y轴不动了,下跳
                    DelayRandom 53,54
                    KeyDown Left
                    DelayRandom 186,187
                    KeyUp Left
                    DelayRandom 22,23
                    stuckTimes = 0
                    cX = -100
                End If
            Else If intX < (tarX-tolLoc) Then //向右走
                Call AntiJam()
                KeyDown Right
                a = (tarX - tolLoc - intX) * 52
                if a < 52 then
                    a = 52
                else if a > 1800 then
                    a = 1800
                End If
                Delay a
                KeyUp Right
            Else If intX > (tarX+tolLoc) Then //向左走
                Call AntiJam()
                KeyDown Left
                a = (intX - tarX - tolLoc) * 52
                if a < 52 then
                    a = 52
                else if a > 1800 then
                    a = 1800
                End If
                Delay a
                KeyUp Left
            else //到Y方向的了
                If intY < (tarY - yTolerance) Then //如果人在目标之上,就下瞬(Y轴是用相反比较符)
                    KeyUp Up
                    If stuckTimes < 2 Then //普通下瞬
                        if cY != intY Then //有变化了,就重置计数器
                            cY = intY
                            stuckTimes = 0
                        else
                            stuckTimes = stuckTimes + 1 //原地就计数器+1
                        End If
                        Call Attack()
                        DelayRandom 115,117
                        KeyDown Down
                        DelayRandom 53,54
                        KeyDown LShift
                        DelayRandom 86,87
                        KeyUp LShift
                        DelayRandom 22,23
                        KeyUp Down
                        DelayRandom 356,360
                    else //计数器达到5次Y轴不动了,下跳
                        stuckTimes = 0
                        KeyDown Down
                        DelayRandom 134,136
                        KeyDown Space
                        if intY > (tarY - 13) //太接近就只用同一种办法
                            DelayRandom 162,164
                        else
                            Delay (tarY - intY) * 49 - 520
                        End If
                        KeyUp Space
                        DelayRandom 66,67
                        KeyUp Down
                        DelayRandom 548,554
                    End If
                Else If intY > (tarY + yTolerance) Then //如果人在目标之下,就上瞬(Y轴是用相反比较符)
                    KeyUp Down
                    If stuckTimes < 2 Then //普通上瞬
                        if cY != intY Then //有变化了,就重置计数器
                            cY = intY
                            stuckTimes = 0
                        else
                            stuckTimes = stuckTimes + 1 //原地就计数器+1
                        End If
                        Call Attack()
                        DelayRandom 115,117
                        KeyDown Up
                        DelayRandom 53,54
                        KeyDown LShift
                        DelayRandom 86,87
                        KeyUp LShift
                        DelayRandom 22,23
                        KeyUp Up
                        DelayRandom 356,360
                    else //计数器达到5次Y轴不动了,钩子
                        csTimes = 0     //启动降落判断
                        cY               = intY
                        stuckTimes       = 0
                        DelayRandom 12,13
                        KeyDown Alt
                        DelayRandom 328,430
                        KeyUp Alt
                        DelayRandom 32,33
                    End If
                else //Y轴搞定了,到容忍度的X轴了,前提是不在空中
                    KeyAllup
                    csTimes = -1 //这一步已经不需要监控Y轴卡住了
                    yTolerance = 0
                    if tolerance == 0 then //传送门型
                        If intX < tarX Then //向右小碎步
                            DelayRandom 45,52
                            KeyDown Right
                            DelayRandom 37,38
                            KeyDown Up
                            DelayRandom 70,71
                            KeyUp Up
                            DelayRandom 37,38
                            Call GetXY()
                            if intX < (alterX-12) or intX > (alterX+12) or intY < (alterY-3) or intY > (alterY+3) then //没到目标
                                KeyDown Up
                                DelayRandom 70,71
                                KeyUp Up
                                DelayRandom 37,38
                            End if
                            Call GetXY()
                            if intX < (alterX-12) or intX > (alterX+12) or intY < (alterY-3) or intY > (alterY+3) then //没到目标
                                KeyDown Up
                                DelayRandom 70,71
                                KeyUp Up
                                DelayRandom 37,38
                            End if
                            Call GetXY()
                            if intX < (alterX-12) or intX > (alterX+12) or intY < (alterY-3) or intY > (alterY+3) then //没到目标
                                KeyDown Up
                                DelayRandom 77,78
                                KeyUp Up
                                DelayRandom 37,38
                            End if
                            Call GetXY()
                            if intX < (alterX-12) or intX > (alterX+12) or intY < (alterY-3) or intY > (alterY+3) then //没到目标
                                KeyDown Up
                                DelayRandom 82,83
                                KeyUp Up
                                DelayRandom 38,39
                            End if
                            KeyUp Right
                            DelayRandom 11,12
                        Else If intX > tarX Then //向左小碎步
                            DelayRandom 45,52
                            KeyDown Left
                            DelayRandom 37,38
                            KeyDown Up
                            DelayRandom 70,71
                            KeyUp Up
                            DelayRandom 37,38
                            Call GetXY()
                            if intX < (alterX-12) or intX > (alterX+12) or intY < (alterY-3) or intY > (alterY+3) then //没到目标
                                KeyDown Up
                                DelayRandom 70,71
                                KeyUp Up
                                DelayRandom 37,38
                            End if
                            Call GetXY()
                            if intX < (alterX-12) or intX > (alterX+12) or intY < (alterY-3) or intY > (alterY+3) then //没到目标
                                KeyDown Up
                                DelayRandom 70,71
                                KeyUp Up
                                DelayRandom 37,38
                            End if
                            Call GetXY()
                            if intX < (alterX-12) or intX > (alterX+12) or intY < (alterY-3) or intY > (alterY+3) then //没到目标
                                KeyDown Up
                                DelayRandom 77,78
                                KeyUp Up
                                DelayRandom 37,38
                            End if
                            Call GetXY()
                            if intX < (alterX-12) or intX > (alterX+12) or intY < (alterY-3) or intY > (alterY+3) then //没到目标
                                KeyDown Up
                                DelayRandom 82,83
                                KeyUp Up
                                DelayRandom 38,39
                            End if
                            KeyUp Left
                            DelayRandom 11,12
                        else //直接按上
                            DelayRandom 45,52
                            KeyDown Up
                            DelayRandom 120,122
                            KeyUp Up
                            DelayRandom 231,234
                        End If
                        Delay 125
                        Call GetXY()
                        if intX >= (alterX-12) and intX <= (alterX+12) and intY >= (alterY-3) and intY <= (alterY+3) then //到了传送后的地点
                            alterX    = 0
                            alterY    = 0
                            Exit Sub
                        else
                        End if
                    else if tolerance == -1 then //负1代表枚举走到目的地
                        a = 0
                        if intX - tarX == 4 then
                            KeyDown Left
                            Delay 197
                            KeyUp Left
                            if facing > 0 then //要求面向右边
                                a = 1
                            end if
                        else if intX - tarX == -4 then
                            KeyDown Right
                            Delay 197
                            KeyUp Right
                            if facing < 0 then //要求面向左边
                                a = 1
                            end if
                        else if intX - tarX == 3 then
                            KeyDown Left
                            Delay 145
                            KeyUp Left
                            if facing > 0 then //要求面向右边
                                a = 1
                            end if
                        else if intX - tarX == -3 then
                            KeyDown Right
                            Delay 145
                            KeyUp Right
                            if facing < 0 then //要求面向左边
                                a = 1
                            end if
                        else if intX - tarX == 2 then
                            KeyDown Left
                            Delay 71
                            KeyUp Left
                            if facing > 0 then //要求面向右边
                                a = 1
                            end if
                        else if intX - tarX == -2 then
                            KeyDown Right
                            Delay 71
                            KeyUp Right
                            if facing < 0 then //要求面向左边
                                a = 1
                            end if
                        else if intX > tarX then
                            KeyDown Left
                            Delay 40
                            KeyUp Left
                            if facing > 0 then //要求面向右边
                                a = 1
                            end if
                        else if intX < tarX then
                            KeyDown Right
                            Delay 40
                            KeyUp Right
                            if facing < 0 then //要求面向左边
                                a = 1
                            end if
                        End If
                        DelayRandom 45,52
                        KeyDown Up
                        DelayRandom 120,122
                        KeyUp Up
                        DelayRandom 231,234
                        Call GetXY()
                        if intX >= (alterX-12) and intX <= (alterX+12) and intY >= (alterY-3) and intY <= (alterY+3) then //到了传送后的地点
                            alterX    = 0
                            alterY    = 0
                            if a > 0 then //要换方向
                                if facing > 0 then  //改面向右边
                                    KeyDown Left
                                    Delay 40
                                    KeyUp Left
                                    DelayRandom 62,65
                                    KeyDown Right
                                    Delay 40
                                    KeyUp Right
                                    DelayRandom 62,65
                                else if facing < 0 then //改面向左边
                                    KeyDown Right
                                    Delay 40
                                    KeyUp Right
                                    DelayRandom 62,65
                                    KeyDown Left
                                    Delay 40
                                    KeyUp Left
                                    DelayRandom 62,65
                                end if
                            End If
                            facing = 0
                            Exit Sub
                        // else
                            // if facing > 0 then  //刚刚有右走
                                // KeyDown Left
                                // Delay 35
                                // KeyUp Left
                                // DelayRandom 62,65
                            // else if facing < 0 then //刚刚是向左走
                                // KeyDown Right
                                // Delay 35
                                // KeyUp Right
                                // DelayRandom 62,65
                            // end if
                        End if
                    else //非传送门
                        if tolerance < 4 Then //容忍度小于4还是要小碎步
                            for 10
                                If intX < (tarX-tolerance) Then //向右小碎步
                                    KeyDown Right
                                    Delay (tarX - tolerance - intX) * 14
                                    KeyUp Right
                                Else If intX > (tarX+tolerance) Then //向左小碎步
                                    KeyDown Left
                                    Delay (intX - tarX - tolerance) * 14
                                    KeyUp Left
                                else
                                    Exit Sub
                                End If

                                Call GetXY() //更新人物坐标
                            Next
                            Exit Sub //次数到了直接结束
                        else //容忍度大于等于4可以直接结束
                            Exit Sub
                        End If
                    End if
                End If
            End If
        End If

        Delay 5
    Wend
End Sub

Sub Navigate //引路
    call GetXY()
    if tarX > -42 and tarX < -21 then //目的在中间
        if intX >= -21 or intX <= -42 then //人物在两边才这么玩
            SavedX		= tarX
            SavedY		= tarY
            SavedTor	= tolerance
            SavedTorY	= yTolerance

            if tarY == 110 then //下面
                tarX       = intX
                tarY       = 110
                tolerance  = 4
                Call Move()
            else if tarY <= 84  //上面
                tarX       = intX
                tarY       = 83
                tolerance  = 4
                yTolerance = 4
                Call Move()
            End If

            tarX		= SavedX
            tarY		= SavedY
            tolerance	= SavedTor
            yTolerance	= SavedTorY
        else if intY > tarY then //人在下面,钩子
            DelayRandom 45,50
            KeyDown LAlt
            DelayRandom 136,138
            KeyUp LAlt
            DelayRandom 622,629
            KeyDown LAlt
            DelayRandom 472,477
            KeyUp LAlt
            DelayRandom 428,433
        else if intY < tarY then //人在上面,下跳
            DelayRandom 45,50
            KeyDown Down
            DelayRandom 76,77
            KeyDown Space
            DelayRandom 1057,1068
            KeyUp Space
            DelayRandom 69,70
            KeyUp Down
            DelayRandom 689,696
        End If
    elseif intX <= -42 and tarX >= -21 then //目的在左边
        SavedX		= tarX
        SavedY		= tarY
        SavedTor	= tolerance
        SavedTorY	= yTolerance

        tarX      = -67
        tarY      = 97
        tolerance = 0
        alterX    = 6 //传送位置
        alterY    = 97
        Call Move()

        tarX		= SavedX
        tarY		= SavedY
        tolerance	= SavedTor
        yTolerance	= SavedTorY
    else if tarX <= -42 and intX >= -21 then //人在最右前往左
        SavedX		= tarX
        SavedY		= tarY
        SavedTor	= tolerance
        SavedTorY	= yTolerance

        tarX      = 6
        tarY      = 97
        tolerance = 0
        alterX    = -67  //传送位置
        alterY    = 97
        Call Move()

        tarX      = SavedX
        tarY      = SavedY
        tolerance = SavedTor
        yTolerance	= SavedTorY
    End If
    Call Move()
End Sub

Sub ReleaseRune //定期解轮 ver2.1
    if GetTimeStamp() - RuneCooldown > 600 then //可以放轮了
        FindPic x1,y1,x2,y2,"E:\MHImg\Rune.bmp",000000,1.00,0,intX,intY
        if intX >= 0 then
            FindPic x1,y1,x2,y2,"E:\MHImg\MapAnchor.bmp",000000,1.00,0,logoX,logoY
            intX = intX - logoX
            intY = intY - logoY - 1 //图标的对准问题
            tarX       = intX
            tarY       = intY
            tolerance  = 2
            yTolerance = 3
            KeyAllup //防错误
            Call Navigate() //解轮也走捷径
            //暂停脚本
            Beep 1000,200
            DelayRandom 100,120
            Beep 1000,200
            DelayRandom 100,120
            Beep 1000,200
            DelayRandom 100,120
            Beep 1000,200
            Pause
            RuneCooldown = GetTimeStamp()
        else
        End If
    End If
End Sub

//Main函数
GetScreenResolution intX, intY
x1             = intX - 1440
y1             = intY - 950
x2             = intX - 1140
y2             = intY - 600
x3             = intX - 640
y3             = intY - 260
xEnd           = intX
yEnd           = intY
InfiTime       = GetTimeStamp() - 110  //上次终极无限的时间
BarlogTime     = 0                     //上次召炎魔的时间
SpellOK        = 1                     //技能冷却
RuneCooldown   = 0                     //解轮时间
stoneSteleTime = 0                     //放轮时间
currentState   = 0                     //初始状态不要去放喷泉
fountainTime   = 0                     //喷泉时间
AuraTime       = 0                     //领域时间
DTime          = 0                     //末日冷却时间
pick1          = GetTimeStamp()        //捡钱中间
pick2          = GetTimeStamp()        //捡钱中间
yTolerance     = 0                     //初始化
Timer          1, 1, 1000              //无限循环
while 1 > 0
    a = GetTimeStamp() - stoneSteleTime
    if a > 570 then //可以放轮了
        DelayRandom 356,358
        KeyDown O
        DelayRandom 337,339
        KeyUp O
        DelayRandom 736,747
        stoneSteleTime = GetTimeStamp()
    End If


    FindPic x3,y3,xEnd,yEnd,"E:\MHImg\Common\1C.bmp",000000,1.00,0,intX,intY
    if currentState = 0 and intX >= 0 then //可以喷泉了
        tarX      = -74
        tarY      = 83
        tolerance = 4
        Call Navigate()
        DelayRandom 56,58
        KeyDown Left
        DelayRandom 62,68
        KeyUp Left
        DelayRandom 106,109
        KeyDown 1
        DelayRandom 194,196
        KeyUp 1
        fountainTime = GetTimeStamp()
        DelayRandom 712,721

        if GetTimeStamp() - pick2 > 108 then
            tarX		= -98
            tarY		= 97
            tolerance	= 6
            Call Navigate()
            pick2 = GetTimeStamp()
        End If
    else
        a = GetTimeStamp() - AuraTime
        if a > 55 Then //前往领域点
            //前往领域点
            tarX      = -27
            tarY      = 110
            tolerance = 6
            Call Navigate()
            DelayRandom 123,130
            KeyDown A
            DelayRandom 130,132
            KeyUp A
            DelayRandom 405,413
            AuraTime = GetTimeStamp()
        else
            if currentState == 1 then //前往楼上
                tarX		= 33
                tarY		= 97
                tolerance	= 6
                Call Navigate()
            else if currentState == 2 then //前往楼上
                tarX		= 32
                tarY		= 84
                tolerance	= 6
                Call Navigate()
            else if currentState == 3  then //前往楼上
                tarX		= 4
                tarY		= 83
                tolerance	= 6
                Call Navigate()

                if GetTimeStamp() - pick1 > 90 then //中间的捡钱
                    currentState = -2
                    pick1 = GetTimeStamp()
                End If
            else if currentState == -1  then //捡钱
                tarX		= -25
                tarY		= 84
                tolerance	= 6
                Call Navigate()
            else
                tarX		= 4
                tarY		= 97
                tolerance	= 6
                Call Navigate()
            End If
            currentState = currentState + 1
            if currentState > 3 then
                currentState = 0
            End If
        End If

        DelayRandom 100,103
    End If
    Call ReleaseRune() //定期解轮


Wend