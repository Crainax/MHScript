Sub TeleportChat //传送门->无限循环对话框
    while 1>0
        FindPic x1,y1,xEnd,yEnd,"E:\MHImg\UI\Dialog\StopChat.bmp|E:\MHImg\UI\Dialog\Next.bmp|E:\MHImg\UI\Dialog\Next_On.bmp",000000,1.00,0,intX,intY
        if intX >= 0 Then
            break
        else
            call GetXY()
            if intY != tarX or intX != tarY then //
                tolerance = -1
                yTolerance = 2
                Call Move_B() //仅移动
            End If
            KeyPress Up
        End If
    Wend
End Sub
Sub NPCChat //传送门->无限循环对话框
