                    while 1>0
                        FindPic x1,y1,xEnd,yEnd,"E:\MHImg\UI\Option_Cancel.bmp",000000,1.00,0,tarX,tarY
                        if tarX >= 0 Then
                            DelayRandom 39,42
                            KeyDown Esc
                            DelayRandom 69,72
                            KeyUp Esc
                            DelayRandom 339,342
                            break
                        else
                            wJam = wJam + 1
                            if wJam > 4 then //重复的动作太多就暂停提醒
                                wJam = 0
                                for 3
                                    Beep 100,200
                                    Beep 200,200
                                    Beep 300,200
                                    Beep 400,200
                                    Beep 500,200
                                Next
                                Pause
                            else
                                DelayRandom 200,300
                                MoveTo intX,intY
                                DelayRandom 200,300
                                LeftClick
                                DelayRandom 339,342

                                MoveTo logoX+6,logoY+213
                                LeftClick
                                DelayRandom 339,342
                                wJam = wJam + 1
                            End If
                        End If
                    Wend

                    break
                else //没找到药水,就打开背包
                    FindPic x1,y1,xEnd,yEnd,"E:\MHImg\UI\Bag\Tag_Coms.bmp",000000,1.00,0,intX,intY
                    if intX >= 0 Then //当前背包标签:消耗品
                        wJam = wJam + 1
                        if wJam > 5 then //重复的动作太多就暂停提醒
                            wJam = 0
                            for 3
                                Beep 100,200
                                Beep 200,200
                                Beep 300,200
                                Beep 400,200
                                Beep 500,200
                            Next
                            Pause
                        else
                            break
                            wJam = wJam + 1
                        End If
                    else
                        FindPic x1,y1,xEnd,yEnd,"E:\MHImg\UI\Bag\Mark.bmp",000000,1.00,0,intX,intY
                        if intX >= 0 Then //切标签页
                            DelayRandom 39,42
                            MoveTo intX+100,intY+50
                            DelayRandom 39,42
                            KeyDown tab
                            DelayRandom 69,72
                            KeyUp tab
                            DelayRandom 339,342
                        else //打开背包
                            for 3
                                DelayRandom 39,42
                                KeyDown i
                                DelayRandom 69,72
                                KeyUp i
                                DelayRandom 39,42
                            Next
                        End If
                    End If
                End If
            Wend

            Call CloseBag()
            Call CloseEquit()
            break
        else
            FindPic x1,y1,xEnd,yEnd,"E:\MHImg\UI\Equit\Tag_1_Off.bmp",000000,1.00,0,intX,intY
            if intX < 0 then //A图没找到就找B图
                FindPic x1,y1,xEnd,yEnd,"E:\MHImg\UI\Equit\Tag_1_On.bmp",000000,1.00,0,intX,intY
            End If
            if intX >= 0 Then
                DelayRandom 39,42
                KeyDown tab
                DelayRandom 69,72
                KeyUp tab
                DelayRandom 39,42
            else
                DelayRandom 39,42
                KeyDown u
                DelayRandom 69,72
                KeyUp u
                DelayRandom 39,42
            End If
        End If
    Wend
