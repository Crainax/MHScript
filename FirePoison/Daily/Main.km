                    LeftClick
                    DelayRandom 139,142
                else
                    FindPic x1,y1,xEnd,yEnd,"E:\MHImg\UI\Dialog\Reject.bmp",000000,1.00,0,intX,intY
                    if intX >= 0 Then //接受
                        MoveTo intX-50,intY
                        LeftClick
                        DelayRandom 139,142
                    else
                        KeyPress PageDown
                    End If
                End If
            End If
        Wend
    End If
End Sub
