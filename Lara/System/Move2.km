Sub AroundS_T //左右释放(带2sCD)
    if GetTimeStamp() - ReleaseTime2 > 2 then
        KeyDown Left
        DelayRandom 45,50
        KeyUp Left
        DelayRandom 45,46
        KeyDown S
        DelayRandom 127,129
        KeyUp S
        DelayRandom 308,312
        KeyDown Right
        DelayRandom 45,50
        KeyUp Right
        DelayRandom 75,76
        KeyDown S
        DelayRandom 135,137
        KeyUp S
        DelayRandom 383,387
        ReleaseTime2 = GetTimeStamp()
    else if GetTimeStamp() - ReleaseTime1 > 2 then
        KeyDown Left
        DelayRandom 45,50
        KeyUp Left
        DelayRandom 45,46
        KeyDown S
        DelayRandom 127,129
        KeyUp S
        DelayRandom 308,312
        KeyDown Right
        DelayRandom 45,50
        KeyUp Right
        DelayRandom 75,76
        KeyDown S
        DelayRandom 135,137
        KeyUp S
        DelayRandom 383,387
        ReleaseTime1 = GetTimeStamp()
    End If
End Sub
