                            End If
                            facing = 0
                            Exit Sub
                        // else
                            // TracePrint 未到达[ & intX & , & intY & ],传送[ & alterX & , & alterY & ] //没到目的点
                            // if facing > 0 then  //刚刚有右走
                                // KeyDown Left
                                // Delay 35
                                // KeyUp Left
                                // DelayRandom 62,65
                            // else if facing < 0 then //刚刚是向左走
                                // KeyDown Right
                                // Delay 35
                                // KeyUp Right
                                // DelayRandom 62,65
                            // end if
                        End if
                    else //非传送门
                        if tolerance < 4 Then //容忍度小于4还是要小碎步
