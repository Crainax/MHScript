                    MouseWheel -1
                    Delay 10
                End If
            Wend

            break
        else
            wJam = wJam + 1
            if wJam > 3 then //重复的动作太多就暂停提醒
                wJam = 0
                Beep 100,200
                Beep 200,200
                Beep 300,200
                Pause
            else
                while 1>0
                    FindPic x1,y1,xEnd,yEnd,"E:\MHImg\Lara\Create\Shop_Mark.bmp",000000,1.00,0,intX,intY
                    if intX >= 0 Then //点击tag1
                        MoveTo intX-179,intY+33
                        LeftClick
                        DelayRandom 139,142
                        MoveTo intX+179,intY-33 //归位不影响判断
                        DelayRandom 139,142
                        break
                    else
                        DelayRandom 39,42
                        KeyDown PageDown
                        DelayRandom 69,72
                        KeyUp PageDown
                        DelayRandom 139,142
                    End If
                Wend
                wJam = wJam + 1
            End If
        End If
    Wend

    while 1>0 //关闭商店
        FindPic x1,y1,xEnd,yEnd,"E:\MHImg\Lara\Create\Shop_Mark.bmp",000000,1.00,0,intX,intY
        if intX < 0 Then
            break
        else
            DelayRandom 39,42
            KeyDown Esc
            DelayRandom 69,72
            KeyUp Esc
            DelayRandom 39,42
        End If
    Wend

    while 1>0 //先处理装备标签
        FindPic x1,y1,xEnd,yEnd,"E:\MHImg\UI\Bag\Tag_Equit.bmp",000000,1.00,0,logoX,logoY
        if logoX >= 0 Then //找到了标签
            //43一个格子
            loopA = 0
            for 6
                loopB = 0
                for 4
                    //9,36
                    MoveTo logoX+9+43*loopB,logoY+36+43*loopA
                    DelayRandom 369,372
                    LeftDoubleClick
                    DelayRandom 339,342 //等一会判断有没有弹窗

                    Call EnterOK()

                    loopB = loopB + 1
                Next
                loopA = loopA + 1
            Next
            break
        else
            FindPic x1,y1,xEnd,yEnd,"E:\MHImg\UI\Bag\Mark.bmp",000000,1.00,0,intX,intY
            if intX >= 0 Then //切标签页
                DelayRandom 39,42
