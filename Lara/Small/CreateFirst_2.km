                DelayRandom 69,72
                KeyUp tab
                DelayRandom 39,42
            else //打开背包
                DelayRandom 39,42
                KeyDown i
                DelayRandom 69,72
                KeyUp i
                DelayRandom 39,42
            End If
        End If
    Wend

    Call CloseBag() //关闭背包

