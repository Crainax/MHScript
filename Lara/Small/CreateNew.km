                else
                    wJam = wJam + 1
                    if wJam > 3 then //重复的动作太多就暂停提醒
                        wJam = 0
                        Beep 100,200
                        Beep 200,200
                        Beep 300,200
                        Pause
                    else
                        DelayRandom 339,342
                        wJam = wJam + 1
                    End If
                End If
            Wend

            break
        else
            FindPic x1,y1,xEnd,yEnd,"E:\MHImg\UI\Equit\Tag_1_Off.bmp",000000,1.00,0,intX,intY
            if intX < 0 then //A图没找到就找B图
                FindPic x1,y1,xEnd,yEnd,"E:\MHImg\UI\Equit\Tag_1_On.bmp",000000,1.00,0,intX,intY
            End If
            if intX >= 0 Then
                DelayRandom 39,42
                KeyDown tab
                DelayRandom 69,72
                KeyUp tab
                DelayRandom 39,42
            else
                DelayRandom 39,42
                KeyDown u
                DelayRandom 69,72
                KeyUp u
                DelayRandom 39,42
            End If
        End If
    Wend


    Call CloseBag() //关闭背包
    Call CloseEquit() //关闭装备栏

End Sub

