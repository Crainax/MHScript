Sub Move_X  //寻敌  (Ver1.3 近距离会发呆的BUG,左右移后直接goto)
    tolerance = 4
    yTolerance = 2
    while 1 > 0
        Call GetXYEnemy()
        if tarY <=0 then //没怪了就离开
            Exit Sub
        End If

        Call GetXY() //先找普通的,再找怪物
        tolLoc = 2 //防跳空
        if intY > tarY +2 and intY <= tarY + 11 and intX >= (tarX-26) and intX <=(tarX+26) then //[特殊情况1]高度小于等于11
            Call Stable()
            if intY == cY then
                if intX < (tarX-4) then //右直跳
                    Delay 77
                    KeyDown Right
                    Delay 101
                    KeyDown Space
                    Delay 101
                    KeyUp Space
                    Delay 183
                    KeyDown Space
                    Delay 113
                    KeyUp Space
                    Delay 42
                    KeyUp Right
                    Delay 500
                else if intX > (tarX+4) //左直跳
                    Delay 77
                    KeyDown Left
                    Delay 101
                    KeyDown Space
                    Delay 101
                    KeyUp Space
                    Delay 183
                    KeyDown Space
                    Delay 113
                    KeyUp Space
                    Delay 42
                    KeyUp Left
                    Delay 500
                else if intX <= tarX then //左反墙跳
                    Delay 45
                    KeyDown Left
                    Delay 119
                    KeyDown Space
                    Delay 122
                    KeyUp Space
                    Delay 91
                    KeyUp Left
                    Delay 64
                    KeyDown Right
                    Delay 76
                    KeyDown Space
                    Delay 131
                    KeyUp Space
                    Delay 112
                    KeyUp Right
                    Delay 500
                else //右反墙跳
                    Delay 45
                    KeyDown Right
                    Delay 119
                    KeyDown Space
                    Delay 122
                    KeyUp Space
                    Delay 91
                    KeyUp Right
                    Delay 64
                    KeyDown Left
                    Delay 76
                    KeyDown Space
                    Delay 131
                    KeyUp Space
                    Delay 112
                    KeyUp Left
                    Delay 500
                end if
            End If
        else if intX < (tarX-12) and intY <= (tarY+25) Then //向右二段跳(Y相差不大就12X,Y太大就24)
            Call RightJumpX()
        else if intX < (tarX-24) and intY > (tarY+25) Then //向右二段跳(Y相差不大就12X,Y太大就24)
            Call RightJumpX()
        Else If intX > (tarX+12) and intY <= (tarY+25) Then //向左二段跳(Y相差不大就12X,Y太大就24)
            Call LeftJumpX()
        Else If intX > (tarX+24) and intY > (tarY+25) Then //向左二段跳(Y相差不大就12X,Y太大就24)
            Call LeftJumpX()
        Else If intX < (tarX-tolLoc) Then
            if intY > (tarY+14) then //向右走(必须钩子情况才左右走)
                Call AntiJam()
                KeyDown Right
                a = (tarX - tolLoc - intX) * 52
                if a < 52 then
                    a = 52
                else if a > 1800 then
                    a = 1800
                End If
                Delay a
                KeyUp Right
                Goto HighJump
            else //小跳平A
                KeyPress Right
                KeyPress Space
                KeyDown F
                DelayRandom 91,92
                KeyUp F
                Delay 500
            End If
        Else If intX > (tarX+tolLoc) Then //向左走(必须钩子情况才左右走)
            if intY > (tarY+14) then //向右走(必须钩子情况才左右走)
                Call AntiJam()
                KeyDown Left
                a = (tarX - tolLoc - intX) * 52
                if a < 52 then
                    a = 52
                else if a > 1800 then
                    a = 1800
                End If
                Delay a
                KeyUp Left
                Goto HighJump
            else //小跳平A
                KeyPress Left
                KeyPress Space
                KeyDown F
                DelayRandom 91,92
                KeyUp F
                Delay 500
            End If
        else
            If intY < (tarY - yTolerance) Then //如果人在目标之上,就下跳(Y轴是用相反比较符)
                Rem HighJump
                Call StableBig()
                if intY == cY then
                    if tarX < intX then
                        KeyPress Left //人在右边
                    else
                        KeyPress Right //人在左边
                    End If
                    KeyDown Down
                    DelayRandom 134,136
                    KeyDown Space
                    if intY > (tarY - 13) //太接近就只用同一种办法
                        DelayRandom 162,164
                    else
                        Delay (tarY - intY) * 49 - 520
                    End If
                    KeyUp Space
                    DelayRandom 66,67
                    KeyUp Down
                    Delay 400
                    KeyPress F //平A新增
                    DelayRandom 148,154
                    while 1>0
                        Call Stable()
                        if intY == cY then
                            Call XSpell()
                            XTime = GetTimeStamp()
                            break
                        End If
                    Wend
                End If
            Else If intY > (tarY + yTolerance) Then //如果人在目标之下,就上跳(Y轴是用相反比较符)
                Call Stable()
                if intY == cY then
                    if tarX < intX then
                        KeyPress Left //人在右边
                    else
                        KeyPress Right //人在左边
                    End If
                    if intY > (tarY+40) Then	//>=再高的话要助跳
                        KeyDown LAlt
                        DelayRandom 161,163
                        KeyUp LAlt
                        DelayRandom 439,444
                        KeyDown LCtrl
                        DelayRandom 108,110
                        KeyUp LCtrl
                        DelayRandom 1414,1432
                        KeyPress F //平A新增
                    else if intY > (tarY+25) Then	//>=26上跳就够不到了
                        KeyDown Ctrl
                        DelayRandom 69,70
                        KeyUp Ctrl
                        DelayRandom 921,931
                        KeyPress F //平A新增
                    else
                        KeyDown LAlt
                        DelayRandom 143,145
                        KeyUp LAlt
                        DelayRandom 769,777
                        KeyPress F //平A新增
                    End If
                    while 1>0
                        Call Stable()
                        if intY == cY then
                            Call XSpell()
                            XTime = GetTimeStamp()
                            break
                        End If
                    Wend
                End If
            End If
        End If
    Wend
End Sub

