Sub AroundS_T //左右释放(带2sCD)
    if GetTimeStamp() - ReleaseTime2 > 2 then
        KeyDown Left
        DelayRandom 45,50
        KeyUp Left
        DelayRandom 45,46
        KeyDown S
        DelayRandom 127,129
        KeyUp S
        DelayRandom 308,312
        KeyDown Right
        DelayRandom 45,50
        KeyUp Right
        DelayRandom 75,76
        KeyDown S
        DelayRandom 135,137
        KeyUp S
        DelayRandom 383,387
        ReleaseTime2 = GetTimeStamp()
    else if GetTimeStamp() - ReleaseTime1 > 2 then
        KeyDown Left
        DelayRandom 45,50
        KeyUp Left
        DelayRandom 45,46
        KeyDown S
        DelayRandom 127,129
        KeyUp S
        DelayRandom 308,312
        KeyDown Right
        DelayRandom 45,50
        KeyUp Right
        DelayRandom 75,76
        KeyDown S
        DelayRandom 135,137
        KeyUp S
        DelayRandom 383,387
        ReleaseTime1 = GetTimeStamp()
    End If
End Sub
Sub Stable //稳定(前面自行getXY)
    cY = intY
    for 3 //确定不动了再跳
        call GetXY()
        if intY == cY Then //Y轴没变化
            Delay 2
        else //还在运动,就停了吧
            break
        End If
    Next
End Sub
Sub StableBig //稳定(前面自行getXY) 大判断
    cY = intY
    for 5 //确定不动了再跳
        call GetXY()
        if intY == cY Then //Y轴没变化
            Delay 2
        else //还在运动,就停了吧
            break
        End If
    Next
End Sub
Sub Move_2  //随机释放的移动 ( Ver3.13+3)
