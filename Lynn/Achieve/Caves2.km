        DelayRandom 54,55
        KeyUp Space
        DelayRandom 69,70
        KeyDown Space
        DelayRandom 62,63
        KeyUp Space
        DelayRandom 53,54
        KeyUp Right
        DelayRandom 143,145
        KeyDown F
        DelayRandom 105,107
        KeyUp F
        DelayRandom 533,539
    else if tarY == 98 and intX >= (-54-4) and intX <=(-54+4) and intY == 98 and tarX >= (-82-8) and tarX <=(-82+8) //右跳去喷泉
        DelayRandom 122,124
        KeyDown Right
        DelayRandom 70,71
        KeyDown Space
        DelayRandom 54,55
        KeyUp Space
        DelayRandom 69,70
        KeyDown Space
        DelayRandom 62,63
        KeyUp Space
        DelayRandom 53,54
        KeyUp Right
        DelayRandom 143,145
        KeyDown F
        DelayRandom 105,107
        KeyUp F
        DelayRandom 533,539
    else if tarX >= -12 and intX <= -52 then //目的在右边
        SavedX		= tarX
        SavedY		= tarY
        SavedTor	= tolerance

        tarX      = -82
        tarY      = 98
        tolerance = 0
        alterX    = 18    //传送位置
        alterY    = 73   //回左门
        Call Move()

        tarX      = SavedX
        tarY      = SavedY
        tolerance = SavedTor
    else if intX >= -12 and tarX <= -52 then //目的在左边
        SavedX		= tarX
        SavedY		= tarY
        SavedTor	= tolerance

        tarX      = 18
        tarY      = 98
        tolerance = 0
        alterX    = -82  //传送位置
        alterY    = 73
        Call Move()

        tarX      = SavedX
        tarY      = SavedY
        tolerance = SavedTor
    End If
    Call Move()
End Sub

Sub ReleaseRune //定期解轮 ver2.1
    if GetTimeStamp() - RuneCooldown > 600 then //可以放轮了
        FindPic x1,y1,x2,y2,"E:\MHImg\Rune.bmp",000000,1.00,0,intX,intY
        if intX >= 0 then
            FindPic x1,y1,x2,y2,"E:\MHImg\MapAnchor.bmp",000000,1.00,0,logoX,logoY
            intX = intX - logoX
            intY = intY - logoY - 1 //图标的对准问题
            tarX       = intX
            tarY       = intY
            tolerance  = 2
            yTolerance = 3
            KeyAllup //防错误
            Call Navigate() //解轮也走捷径
            //暂停脚本
            Beep 1000,200
            DelayRandom 100,120
            Beep 1000,200
            DelayRandom 100,120
            Beep 1000,200
            DelayRandom 100,120
            Beep 1000,200
            Pause
            RuneCooldown = GetTimeStamp()
        else
        End If
    End If
End Sub

