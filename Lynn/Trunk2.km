Sub Navigate //引路
    call GetXY()
    if tarY == 127 and tarX >= (43-4) and tarX <=(43+4) then //前往喷泉
        if intY >= 125 and intX >= -41 and intX <= 2 then //下传送门
            SavedX		= tarX
            SavedY		= tarY
            SavedTor	= tolerance

            if GetTimeStamp() - pick1 > 110 then //捡钱
                tarX      = -84
                tarY      = 128
                tolerance = 0
                alterX    = -7  //传送位置
                alterY    = 139
                Call Move()
                pick1 = GetTimeStamp()
            End If

            tarX      = -7
            tarY      = 139
            tolerance = 0
            alterX    = 49  //传送位置
            alterY    = 109
            Call Move()

            tarX      = SavedX
            tarY      = SavedY
            tolerance = SavedTor
        else if intX < -41 then //左传送门
            SavedX		= tarX
            SavedY		= tarY
            SavedTor	= tolerance

            tarX      = -84
            tarY      = 128
            tolerance = 0
            alterX    = -7  //传送位置
            alterY    = 139
            Call Move()

            tarX      = -7
            tarY      = 139
            tolerance = 0
            alterX    = 49  //传送位置
            alterY    = 109
            Call Move()

            tarX      = SavedX
            tarY      = SavedY
            tolerance = SavedTor
        End If

        call GetXY()
        if intY == 109 and intX >= (49-4) and intX <=(49+4) then //
            DelayRandom 102,109
            KeyDown Down
            DelayRandom 87,88
            KeyDown Space
            DelayRandom 70,71
            KeyUp Space
            DelayRandom 37,38
            KeyUp Down
            DelayRandom 109,111
            KeyDown F
            DelayRandom 128,130
            KeyUp F
            DelayRandom 400,405
            KeyDown F
            DelayRandom 95,96
            KeyUp F
            DelayRandom 363,367
        End If
    else if tarY == 125 and tarX >= (-9-4) and tarX <=(-9+4) and intY == 127 and intX >= (43-4) and intX <=(43+4) then //前往平A点
        DelayRandom 82,83
        KeyDown Left
        DelayRandom 137,139
        KeyDown Space
        DelayRandom 62,63
        KeyUp Space
        DelayRandom 62,63
        KeyDown Space
        DelayRandom 77,78
        KeyUp Space
        DelayRandom 102,104
        KeyDown LShift
        DelayRandom 104,106
        KeyUp LShift
        DelayRandom 195,197
        KeyDown Space
        DelayRandom 194,196
        KeyUp Space
        DelayRandom 87,88
        KeyUp Left
        DelayRandom 82,83
        KeyDown F
        DelayRandom 120,122
        KeyUp F
        DelayRandom 269,272
    End If

    call GetXY()
    if intY != tarY or intX < (tarX-tolerance) or intX > (tarX+tolerance) then
        Call Move()
    End If
End Sub

Sub ReleaseRune //定期解轮 ver2.1
    if GetTimeStamp() - RuneCooldown > 600 then //可以放轮了
        FindPic x1,y1,x2,y2,"E:\MHImg\Rune.bmp",000000,1.00,0,intX,intY
        if intX >= 0 then
            FindPic x1,y1,x2,y2,"E:\MHImg\MapAnchor.bmp",000000,1.00,0,logoX,logoY
            intX = intX - logoX
            intY = intY - logoY - 1 //图标的对准问题
            tarX       = intX
            tarY       = intY
            tolerance  = 2
            yTolerance = 3
            KeyAllup //防错误
            Call Navigate() //解轮也走捷径
            //暂停脚本
            Beep 1000,200
            DelayRandom 100,120
            Beep 1000,200
            DelayRandom 100,120
            Beep 1000,200
            DelayRandom 100,120
            Beep 1000,200
            Pause
            RuneCooldown = GetTimeStamp()
        else
        End If
    End If
End Sub

