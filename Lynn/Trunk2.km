                                Call GetXY() //更新人物坐标
                            Next
                            Exit Sub //次数到了直接结束
                        else //容忍度大于等于4可以直接结束
                            Exit Sub
                        End If
                    End if
                End If
            End If
        End If
        Delay 5
    Wend
End Sub

Sub Navigate //引路
    call GetXY()
    if tarY == 127 and tarX >= (43-4) and tarX <=(43+4) then //前往喷泉
        if intY >= 125 and intX >= -41 and intX <= 2 then //下传送门
            SavedX		= tarX
            SavedY		= tarY
            SavedTor	= tolerance

            tarX      = -7
            tarY      = 139
            tolerance = 0
            alterX    = 49  //传送位置
            alterY    = 109
            Call Move()

            tarX      = SavedX
            tarY      = SavedY
            tolerance = SavedTor
        else if intX < -41 then //左传送门
            SavedX		= tarX
            SavedY		= tarY
            SavedTor	= tolerance

            tarX      = -84
            tarY      = 128
            tolerance = 0
            alterX    = -7  //传送位置
            alterY    = 139
            Call Move()

            tarX      = -7
            tarY      = 139
            tolerance = 0
            alterX    = 49  //传送位置
            alterY    = 109
            Call Move()

            tarX      = SavedX
            tarY      = SavedY
            tolerance = SavedTor
        End If

        call GetXY()
        if intY == 109 and intX >= (49-4) and intX <=(49+4) then //
            DelayRandom 62,68
            KeyDown Left
            DelayRandom 62,68
            KeyUp Left
            DelayRandom 62,68
            KeyDown Down
            DelayRandom 87,88
            KeyDown Space
            DelayRandom 70,71
            KeyUp Space
            DelayRandom 37,38
            KeyUp Down
            DelayRandom 109,111
            KeyDown F
            DelayRandom 128,130
            KeyUp F
            DelayRandom 400,405
            KeyDown F
            DelayRandom 95,96
            KeyUp F
            DelayRandom 363,367
        End If
    else if tarY == 125 and tarX >= (-9-4) and tarX <=(-9+4) and intY == 127 and intX >= (43-4) and intX <=(43+4) then //前往平A点
        DelayRandom 82,83
