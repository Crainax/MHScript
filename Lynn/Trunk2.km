        DelayRandom 137,139
        KeyDown Space
        DelayRandom 62,63
        KeyUp Space
        DelayRandom 62,63
        KeyDown Space
        DelayRandom 77,78
        KeyUp Space
        DelayRandom 102,104
        KeyDown LShift
        DelayRandom 104,106
        KeyUp LShift
        DelayRandom 195,197
        KeyDown Space
        DelayRandom 194,196
        KeyUp Space
        DelayRandom 87,88
        KeyUp Left
        DelayRandom 82,83
        KeyDown F
        DelayRandom 120,122
        KeyUp F
        DelayRandom 269,272
