                        Delay 111
                        KeyUp F
                        Delay 362
                        Call XSpell()
                    else if intY > (tarY+26) Then	//>=27以上才用钩子法
                        TracePrint hooking
                        KeyDown LAlt
                        DelayRandom 69,70
                        KeyUp LAlt
                        DelayRandom 721,731
                        KeyPress F
                        Delay 200
                        Call XSpell()
                    else if intY > (tarY+18) Then	//>=19以上就助跳再斜上冲
                        DelayRandom 15,25
                        KeyDown Left
                        Delay 47
                        KeyDown Space
                        Delay 53
                        KeyUp Space
                        Delay 53
                        KeyDown Space
                        Delay 88
                        KeyUp Space
                        Delay 46
