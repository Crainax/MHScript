SpellOK = 0
while 1 > 0
    LEDState = GetLED(1)
    if LEDState == 2 then // (利曼世界的眼泪) 左右单点跳A测转身延迟
        while 1 > 0
            tarX       = 3
            tarY       = 93
            tolerance  = 2
            yTolerance = 0
            Call Move()

            tarX       = 32
            tarY       = 93
            tolerance  = 2
            yTolerance = 0
            Call Move()
        Wend
    else if LEDState == 3 then // (利曼世界的眼泪) 左右跳3下A测Spell的延迟
        while 1 > 0
            tarX       = -37
            tarY       = 93
            tolerance  = 4
            yTolerance = 0
            Call Move()

            tarX       = 49
            tarY       = 93
            tolerance  = 4
            yTolerance = 0
            Call Move()
        Wend
    elseif LEDState == 0 then // 标准公式,测试完后改回0
        tolLoc = 1
