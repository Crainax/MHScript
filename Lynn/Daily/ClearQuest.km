Sub SubmitQuest //提交任务
    wJam = 0 //放在循环外面位置
    while 1>0
        FindPic x1,y1,xEnd,yEnd,"E:\MHImg\UI\Daily\Quest\MarkA.bmp",000000,1.00,0,intX,intY
        if intX >= 0 Then
            wJam = 0 //放在成功的位置
            Call ClearDialog()
        else
            wJam = wJam + 1
            if wJam > 5 then //多次失败代表任务接完了
                Call ClearDialog()
                break
            else
                KeyPress =
                MoveR 50,50
                Delay 200
            End If
        End If
    Wend
End Sub

