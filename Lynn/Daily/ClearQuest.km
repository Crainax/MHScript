End Sub

Sub ReceiveQuest //领每日任务
    while 1>0 //接ARC的
        FindPic x1,y1,xEnd,yEnd,"E:\MHImg\UI\Daily\Quest\Mark1.bmp",000000,1.00,0,intX,intY
        if intX >= 0 Then
            MoveTo intX+50,intY+5
            LeftClick
            Delay 300

            while 1>0
                FindPic x1,y1,xEnd,yEnd,"E:\MHImg\UI\Dialog\StopChat.bmp|E:\MHImg\UI\Dialog\No2.bmp|E:\MHImg\UI\Dialog\No2B.bmp",000000,1.00,0,intX,intY
                if intX >= 0 Then
                    Call ClearDialog()
                    break
                else
                    Delay 200
                End If
            Wend
            break
        else
            KeyPress =
            MoveR 50,50
            Delay 200
        End If
    Wend
    while 1>0 //接AUT的
        FindPic x1,y1,xEnd,yEnd,"E:\MHImg\UI\Daily\Quest\Mark2.bmp",000000,1.00,0,intX,intY
        if intX >= 0 Then
            MoveTo intX+50,intY+5
            LeftClick
            Delay 300

            while 1>0
                FindPic x1,y1,xEnd,yEnd,"E:\MHImg\UI\Dialog\StopChat.bmp|E:\MHImg\UI\Dialog\No2.bmp|E:\MHImg\UI\Dialog\No2B.bmp",000000,1.00,0,intX,intY
                if intX >= 0 Then
                    Call ClearDialog()
                    break
                else
                    Delay 200
                End If
            Wend
            break
        else
            KeyPress =
            MoveR 50,50
            Delay 200
        End If
    Wend
    wJam = 0 //放在循环外面位置
    while 1>0 //接其他的
        FindPic x1,y1,xEnd,yEnd,"E:\MHImg\UI\Daily\Quest\Mark3.bmp",000000,1.00,0,intX,intY
        if intX >= 0 Then
            wJam = 0 //放在成功的位置
            MoveTo intX+50,intY+5
            LeftClick
            Delay 300

            while 1>0
                FindPic x1,y1,xEnd,yEnd,"E:\MHImg\UI\Dialog\StopChat.bmp",000000,1.00,0,intX,intY
                if intX >= 0 Then
                    Call ClearDialog()
                    break
                else
                    Delay 200
                End If
            Wend
        else
            wJam = wJam + 1  //循环内部
            if wJam > 6 then //三次失败,
                Call ClearDialog()
                break
            else
                KeyPress =
                MoveR 20,0
                Delay 200
            End If
        End If
    Wend
End Sub


