                    KeyUp Space
                    DelayRandom 66,67
                    KeyUp Down
                    DelayRandom 548,554
                End If
            Else If intY > (tarY + yTolerance) Then //如果人在目标之下,上钩
                Call Stable()
                if intY == cY then
                    if intY > (tarY+40) Then	//>=40以上要先跳再钩子
                        DelayRandom 15,25
                        KeyDown Up
                        Delay 80
                        KeyDown Space
                        Delay 52
                        KeyUp Space
                        Delay 73
                        KeyDown Space
                        Delay 92
                        KeyUp Space
                        Delay 69
                        KeyUp Up
                        Delay 52
                        Delay 104
                        Delay 401
